              "title": null,
              "semantic_type": "TextAtom",
              "is_public": true,
              "text": " - <span class=\"mathquill\">x_2=2</span>\n - <span class=\"mathquill\">m_{12}=5</span>\n - <span class=\"mathquill\">m_{21}=8</span>",
              "instructor_notes": ""
            },
